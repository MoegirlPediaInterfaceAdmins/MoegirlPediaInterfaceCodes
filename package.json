        "duplexify": "^4.1.3",
        "envify": "^4.1.0",
        "eslint": "^9.39.2",
        "eslint-plugin-prefer-arrow-functions": "^3.9.1",
        "esmify": "^2.1.1",
        "hash-wasm": "^4.12.0",
        "ip": "^2.0.1",
        "jsonc-parser": "^3.3.1",
